	<script>
	function eliminarValoresAnteriores() {
	  var radios = document.getElementsByClassName("radio-option");
	  for (var i = 0; i < radios.length; i++) {
	    radios[i].value = "";
	  }
	}

	function mostrarValores(radio) {
	  var valores = radio.value.split(",");
	
	  var divValor1 = document.getElementById("Valor1");
	  var divValor2 = document.getElementById("Valor2");
	  var divValor3 = document.getElementById("Valor3");
	
	  if (valores.length === 3) {
	    divValor1.innerHTML = valores[0];
	    divValor2.innerHTML = valores[1];
	    divValor3.innerHTML = valores[2];
	  } else {
	    divValor1.innerHTML = valores[0];
	    divValor2.innerHTML = valores[1];
	    divValor3.innerHTML = "";
	  }

	  eliminarValoresAnteriores();
	}



		var contenedorTexto = document.getElementById("contenedorTexto");
		var opciones = document.getElementsByName("opcion");
		var ultimoParrafo = null;

		for (var i = 0; i < opciones.length; i++) {
		    opciones[i].addEventListener("change", function() {
		        if (this.checked) {
		            if (!ultimoParrafo) {
		                ultimoParrafo = document.createElement("p");
		                contenedorTexto.appendChild(ultimoParrafo);
		            }
		            ultimoParrafo.innerHTML += this.value + "<br>";
		        } else {
		            ultimoParrafo.innerHTML = ultimoParrafo.innerHTML.replace(this.value + "<br>", "");
		            if (ultimoParrafo.innerHTML.trim() === "") {
		                contenedorTexto.removeChild(ultimoParrafo);
		                ultimoParrafo = null;
		            }
		        }
		    });
		}

		var contenedorTexto3 = document.getElementById("contenedorTexto3");
		var opciones3 = document.getElementsByName("opcion3");
		var parrafo3 = null;

		for (var i = 0; i < opciones3.length; i++) {
		    opciones3[i].addEventListener("change", function() {
		        if (this.checked) {
		            if (parrafo3) {
		                contenedorTexto3.removeChild(parrafo3);
		            }
		            parrafo3 = document.createElement("p");
		            parrafo3.innerHTML = this.value;
		            contenedorTexto3.appendChild(parrafo3);
		        }
		    });
		}
		var fechafin = document.getElementById("fechafin");
		var desconegut = document.getElementById("desconegut");
			
		desconegut.addEventListener("change", function() {
		  if (this.checked) {
		    fechafin.disabled = true;
		  } else {
		    fechafin.disabled = false;
		  }
});

function copyToClipboard(html) {
  const data = [new ClipboardItem({ "text/html": new Blob([html], { type: "text/html" }) })];
  navigator.clipboard.write(data).then(
    function() {
      console.log("Contenido copiado al portapapeles");
    },
    function() {
      console.log("Error al copiar el contenido");
    }
  );
}

function copyContent() {
  var inicio = new Date(document.getElementById("fechainicio").value);
  var des = document.getElementById("desconegut").checked ? "Desconegut" : "";
  var fin = des !== "Desconegut" ? new Date(document.getElementById("fechafin").value) : null;

  var diaInicio = inicio.getDate().toString().padStart(2, "0");
  var mesInicio = (inicio.getMonth() + 1).toString().padStart(2, "0");
  var anioInicio = inicio.getFullYear().toString();
  var horaInicio = inicio.getHours().toString().padStart(2, "0");
  var minutoInicio = inicio.getMinutes().toString().padStart(2, "0");

  var diaFin = fin ? fin.getDate().toString().padStart(2, "0") : "";
  var mesFin = fin ? (fin.getMonth() + 1).toString().padStart(2, "0") : "";
  var anioFin = fin ? fin.getFullYear().toString() : "";
  var horaFin = fin ? fin.getHours().toString().padStart(2, "0") : "";
  var minutoFin = fin ? fin.getMinutes().toString().padStart(2, "0") : "";

  var contenido =
    Valor1.innerHTML +
	"<br>" +
	"<br>" +
	Valor3.innerHTML +
	"<br>" +
	"<br>" +
    "<b>Inici:</b> " +
    diaInicio +
    "/" +
    mesInicio +
    "/" +
    anioInicio +
    " " +
    horaInicio +
    ":" +
    minutoInicio +
    (fin
      ? "<br><b>Fi:</b> " +
        diaFin +
        "/" +
        mesFin +
        "/" +
        anioFin +
        " " +
        horaFin +
        ":" +
        minutoFin
      : "") +
    (des ? "<br><b>Fi: </b> " + des : "") +
    "<br>" +
	"<br>" +
    "<b>" +
    Valor2.innerHTML +
    "</b>" +
    contenedorTexto.innerHTML +
    contenedorTexto3.innerHTML +
	"<br>"+
    atencio012.innerHTML +
	"<br>"+
	"<br>"+
	"<b>CAUSA DE LA INDISPONIBILITAT: Comunicat " +
    document.querySelector(".select").value +
	":</b>" +
	causa.value +
	"<br>"+
	indisponible.innerHTML ;

  copyToClipboard(contenido);
}




